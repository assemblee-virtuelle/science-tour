{
    "name": "root/st0",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
	    "TheScienceTour\\": "src/TheScienceTour"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.0.0",
        "adesigns/calendar-bundle": "dev-master",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/mongodb-odm": "^1.1",
        "doctrine/mongodb-odm-bundle": "^3.3",
        "doctrine/orm": "^2.4.8",
        "egeloen/google-map-bundle": "3.0.1",
        "exercise/htmlpurifier-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "2.2.0",
        "friendsofsymfony/user-bundle": "2.0.1",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "dev-master",
        "jms/security-extra-bundle": "^1.6",
        "knplabs/knp-paginator-bundle": "^2.6",
        "kriswallsmith/buzz": "^0.15.0",
        "leafo/lessphp": "^0.5.0",
        "mongodb/mongodb": "^1.1",
        "nelmio/api-doc-bundle": "^2.13",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/doctrine-mongodb-admin-bundle": "dev-master",
        "sonata-project/intl-bundle": "^2.3",
        "sonata-project/media-bundle": "^3.5",
        "stfalcon/tinymce-bundle": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/swiftmailer-bundle": "~2.3,>=2.3.10",
        "symfony/symfony": "2.8.*",
        "twig/twig": "^1.0||^2.0",
        "willdurand/geocoder": "^3.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "mininum-stability": "dev",
    "prefer-stable": true,
    "provide": {
	"ext-mongo": "1.6.14"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
