{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% if value %}
        {% if field_description.hasAssociationAdmin and field_description.associationadmin.isGranted('EDIT') and field_description.associationadmin.hasRoute('edit') %}
            <a href="{{ field_description.associationadmin.generateObjectUrl('edit', value) }}">{{ value }}</a>{% if value.labelType %} ({{ value.labelType }}){% endif %}
        {% else %}
        	{{ value }}{% if value.labelType %} ({{ value.labelType }}){% endif %}
        {% endif %}
    {% endif %}
{% endblock %}